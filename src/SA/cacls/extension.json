{
    "name": "Cacls (SA)",
    "version": "0.0.0",
    "command_name": "sa-cacls",
    "extension_author": "moloch--",
    "original_author": "TrustedSec",
    "repo_url": "https://github.com/sliverarmory/CS-Situational-Awareness-BOF",
    "help": "Lists user permissions for the specified file, wildcards supported",
    "depends_on": "coff-loader",
    "entrypoint": "go",
    "files": [
        {
            "os": "windows",
            "arch": "amd64",
            "path": "cacls.x64.o"
        },
        {
            "os": "windows",
            "arch": "386",
            "path": "cacls.x86.o"
        }
    ],
    "arguments": [
        {
            "name": "filepath",
            "desc": "The file path to list permissions for",
            "type": "string",
            "optional": false
        }
    ]
}
