{
    "name": "List Modules (SA)",
    "version": "0.0.0",
    "command_name": "sa-listmods",
    "extension_author": "moloch--",
    "original_author": "TrustedSec",
    "repo_url": "https://github.com/sliverarmory/CS-Situational-Awareness-BOF",
    "help": "List a process' modules (DLL). Target current process if pid is empty. Complement to driversigs to determine if our process was injected by edr/av.",
    "depends_on": "coff-loader",
    "entrypoint": "go",
    "files": [
        {
            "os": "windows",
            "arch": "amd64",
            "path": "listmods.x64.o"
        },
        {
            "os": "windows",
            "arch": "386",
            "path": "listmods.x86.o"
        }
    ],
    "arguments": [
        {
            "name": "pid",
            "desc": "PID",
            "type": "integer",
            "optional": true
        }
    ]
}
