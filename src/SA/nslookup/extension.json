{
    "name": "NSLookup (SA)",
    "version": "0.0.0",
    "command_name": "sa-nslookup",
    "extension_author": "moloch--",
    "original_author": "TrustedSec",
    "repo_url": "https://github.com/sliverarmory/CS-Situational-Awareness-BOF",
    "help": "Makes a dns query. NOTE: Some situations are limited due to observed crashes",
    "long_help": "",
    "depends_on": "coff-loader",
    "entrypoint": "go",
    "files": [
        {
            "os": "windows",
            "arch": "amd64",
            "path": "nslookup.x64.o"
        },
        {
            "os": "windows",
            "arch": "386",
            "path": "nslookup.x86.o"
        }
    ],
    "arguments": [
        {
            "name": "hostname",
            "desc": "Hostname to resolve",
            "type": "string",
            "optional": false
        },
        {
            "name": "server",
            "desc": "DNS server ",
            "type": "string",
            "optional": true
        },
        {
            "name": "type",
            "desc": "DNS record type (A, AAAA, or ANY)",
            "type": "string",
            "optional": true
        }
    ]
}