{
    "name": "Reg Query (SA)",
    "version": "0.0.0",
    "command_name": "sa-reg-query",
    "extension_author": "moloch--",
    "original_author": "TrustedSec",
    "repo_url": "https://github.com/sliverarmory/CS-Situational-Awareness-BOF",
    "help": "Query the Windows registry",
    "long_help": "",
    "depends_on": "coff-loader",
    "entrypoint": "go",
    "files": [
        {
            "os": "windows",
            "arch": "amd64",
            "path": "reg-query.x64.o"
        },
        {
            "os": "windows",
            "arch": "386",
            "path": "reg-query.x86.o"
        }
    ],
    "arguments": [
        {
            "name": "hostname",
            "desc": "Hostname to resolve",
            "type": "string",
            "optional": true
        },
        {
            "name": "hive",
            "desc": "Hive identifier: 0 = HKCL, 1 = HKCU, 2 = HKLM, 3 = HKU",
            "type": "integer",
            "optional": false
        },
        {
            "name": "path",
            "desc": "Registry key path",
            "type": "string",
            "optional": false
        },
        {
            "name": "key",
            "desc": "Registry key name",
            "type": "string",
            "optional": true
        },
        {
            "name": "recursive",
            "desc": "Recursive search",
            "type": "integer",
            "optional": true
        }
    ]
}